name: CustomDNS
desc: 自定义DNS配置项
category: "General"
icon: https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Drill.png
dns:
  ipv6: false
  enable: true
  follow-rule: true
  default-nameserver:
  - 180.76.76.76
  enhanced-mode: fake-ip
  fake-ip-range: 192.18.0.1/16
  fake-ip-filter: 
    - '*.lan'
    - 'engage.nanocat.me'
    - '*.apple.com'
  nameserver:
    - https://dns.alidns.com/dns-query
    - https://doh.pub/dns-query
  fallback:
    - https://8.8.4.4/dns-query
    - https://1.0.0.1/dns-query
  fallback-filter:
    geoip: true
    geoip-code: CN
    ipcidr:
      - 240.0.0.0/4
    domain:
      - '+.youtube.com'
      - '+.ytimg.com'
  nameserver-policy:
    '+.qq.com': '119.29.29.29'
    '+.weixin.com': '119.28.28.28'
    '*.tencent.com': '119.28.28.28'
    '*.wechat.com': '119.28.28.28'
    '+.taobao.com': '223.6.6.6'
    '+.tmall.com': '223.6.6.6'
    '+.hostloc.com': 'https://1.0.0.1/dns-query'
    '+.bing.com': 'https://1.0.0.1/dns-query'
    '+.microsoft.com': 'https://1.0.0.1/dns-query'
    '+.google.com': 'https://1.0.0.1/dns-query'
    '+.googlevideo.com': 'https://1.0.0.1/dns-query'
    '+.githubusercontent.com': 'https://1.0.0.1/dns-query'
    '+.vscode-cdn.net': 'https://1.0.0.1/dns-query'
    '+.github.dev': 'https://1.0.0.1/dns-query'
    '+.dropbox.com': 'https://1.0.0.1/dns-query'
    '+.youtube.com': 'https://1.0.0.1/dns-query'
    '+.v2ex.com': https://1.0.0.1/dns-query
    '+.smallstrawberry.com': 'https://1.0.0.1/dns-query'
    '+.vimeo.com': 'https://1.0.0.1/dns-query'
    '+.cloudflare.com': 'https://1.0.0.1/dns-query'
    '+.claude.ai': 'https://1.0.0.1/dns-query'
    '+.github.com': 'https://1.0.0.1/dns-query'
    '+.twimg.com': 'https://1.0.0.1/dns-query'
    '+.twitter.com': 'https://1.0.0.1/dns-query'
    't.co': 'https://1.0.0.1/dns-query'
